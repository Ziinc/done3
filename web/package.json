    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.19",
    "@supabase/supabase-js": "^2.1.3",
    "antd": "^5.0.5",
    "axios": "^1.7.2",
    "lucide-react": "^0.105.0-alpha.4",
    "marked": "^4.2.5",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "swr": "^2.2.5",
